{
  "name": "react-template",
  "private": true,
  "version": "1.0.0-rc.1",
  "gitHooks": {
    "pre-commit": "tsc && yarn lint",
    "pre-push": "yarn test && yarn rsc"
  },
  "scripts": {
    "vite-dev-server": "vite",
    "vite-build": "vite build",
    "start": "cross-env VITE_APP_ENVIRONMENT=development yarn vite-dev-server",
    "build": "cross-env VITE_APP_ENVIRONMENT=production yarn vite-build",
    "analyze": "cross-env VITE_APP_ENVIRONMENT=analyze yarn vite-build",
    "test": "yarn test:unit",
    "test:unit": "vitest run --passWithNoTests",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{css,sass,scss}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{js,ts,jsx,tsx}\" --cache --fix --max-warnings=0",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^1.2.2",
    "bignumber.js": "^9.0.2",
    "dayjs": "^1.11.3",
    "downshift": "^7.1.2",
    "framer-motion": "^8.4.0",
    "i18next": "^22.4.9",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.0",
    "mitt": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.6.2",
    "react-toastify": "^9.0.5",
    "react-use": "^17.4.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@comparaonline/stylelint-config-scss-modules": "^1.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@testing-library/react": "^13.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^3.0.1",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "happy-dom": "^8.1.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.54.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.6.3",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.0",
    "yorkie": "^2.0.0"
  }
}
